<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="PRProjMulticamCreator.MainWindow"
        Title="PRProjMulticamCreator">

    <StackPanel Orientation="Vertical">
        <TextBlock Text="Привет! Сейчас будем обрабатывать файл"/>
        <Button Content="Выбрать файл" Click="SelectProjFileButton_Click" />
        <TextBlock x:Name="SelectedFile"/>
        <Button Name="StartButton" Content="Начать анализ" Click="StartButton_Click" />
        <ProgressBar x:Name="ProgressBar" IsVisible="False" IsIndeterminate="True" Margin="10"/>
        <TextBlock x:Name="Result"/>
    </StackPanel>
</Window>
